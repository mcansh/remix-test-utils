{
  "name": "remix-stubs",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "2.7.1",
    "turbo": "1.6.3",
    "typescript": "4.8.4",
    "vite": "3.2.2",
    "vitest": "0.24.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.2.1"
}
